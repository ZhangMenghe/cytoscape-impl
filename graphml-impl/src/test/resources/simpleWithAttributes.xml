<?xml version="1.0" encoding="UTF-8"?>
<graphml xmlns="http://graphml.graphdrawing.org/xmlns"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://graphml.graphdrawing.org/xmlns 
        http://graphml.graphdrawing.org/xmlns/1.0/graphml.xsd">
	<key id="d0" for="node" attr.name="color" attr.type="string">
		<default>yellow</default>
	</key>
	<key id="d1" for="edge" attr.name="weight" attr.type="double" />
	<key id="d2" for="node" attr.name="rank" attr.type="int" />
	<key id="d3" for="node" attr.name="degree" attr.type="long" />
	<key id="d4" for="node" attr.name="score" attr.type="float" />
	<key id="d5" for="node" attr.name="tagged" attr.type="boolean">
		<default>false</default>
	</key>
	
	<!-- Network Table Data -->
	<key id="d6" for="graph" attr.name="name" attr.type="string">
		<default>Sample Network</default>
	</key>
	<key id="d7" for="graph" attr.name="number_test" attr.type="double">
		<default>1.234</default>
	</key>
	
	<graph id="G" edgedefault="undirected">
		<data key="d6">Small GraphML Network</data>
		<data key="d7">100.123</data>

		<node id="n0">
			<data key="d0">green</data>
			<data key="d2">3</data>
			<data key="d3">2</data>
			<data key="d4">0.95</data>
			<data key="d5">true</data>
		</node>
		<node id="n1" />
		<node id="n2">
			<data key="d0">blue</data>
			<data key="d2">1</data>
			<data key="d3">3</data>
			<data key="d4">0.2</data>
			<data key="d5">false</data>
		</node>
		<node id="n3">
			<data key="d0">red</data>
			<data key="d2">2</data>
			<data key="d3">3</data>
			<data key="d4">0.8</data>
			<data key="d5">false</data>
		</node>
		<node id="n4">
			<data key="d3">2</data>
		</node>
		<node id="n5">
			<data key="d0">turquoise</data>
			<data key="d3">2</data>
			<data key="d5">false</data>
		</node>
		<edge id="e0" source="n0" target="n2">
			<data key="d1">1.0</data>
		</edge>
		<edge id="e1" source="n0" target="n1">
			<data key="d1">1.0</data>
		</edge>
		<edge id="e2" source="n1" target="n3">
			<data key="d1">2.0</data>
		</edge>
		<edge id="e3" source="n3" target="n2" />
		<edge id="e4" source="n2" target="n4" />
		<edge id="e5" source="n3" target="n5" />
		<edge id="e6" source="n5" target="n4">
			<data key="d1">1.1</data>
		</edge>
	</graph>
</graphml>